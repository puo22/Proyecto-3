# Prueba básica de DeepLang

a = 10
b = 20
c = a + b * 2
print(c)

# Listas
nums = [1,2,3,4]
print(len(nums))

# Condicional
if c > 30:
    print("c es mayor a 30")
else:
    print("c NO es mayor a 30")

# Ciclo while
i = 0
while i < 3:
    print(i)
    i = i + 1

# Ciclo for
for x in nums:
    print(x)

# Función usuario
def sumar(x, y):
    r = x + y
    return r

print( sumar(5, 7) )
